pipeline
{
  agent any
  stages{
      stage("Code Checkout"){
          git branch: 'devops', url: 'https://github.com/gauravjadhav8/my-app'
      }
      stage("Compile-Package"){
          sh "mvn clean package"
      }
      stage("Push to DockerHub"){
          sh "docker build -t . peace_myweb_new"
      }
   }
}
